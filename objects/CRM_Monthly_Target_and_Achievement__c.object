    <validationRules>
        <fullName>Validate_Month</fullName>
        <active>false</active>
        <description>Please select Month according to KPI&apos;s selected quarter.</description>
        <errorConditionFormula>AND(
OR(
AND(
!ISPICKVAL(CRM_KPI__r.CRM_Quarter__c, &apos;1&apos;),
OR(
ISPICKVAL(CRM_Month__c, &apos;APRIL&apos;),
ISPICKVAL(CRM_Month__c, &apos;MAY&apos;),
ISPICKVAL(CRM_Month__c, &apos;JUNE&apos;)
)
),
AND(
!ISPICKVAL(CRM_KPI__r.CRM_Quarter__c, &apos;2&apos;),
OR(
ISPICKVAL(CRM_Month__c, &apos;JULY&apos;),
ISPICKVAL(CRM_Month__c, &apos;AUGUST&apos;),
ISPICKVAL(CRM_Month__c, &apos;SEPTEMBER&apos;)
)
),
AND(
!ISPICKVAL(CRM_KPI__r.CRM_Quarter__c, &apos;3&apos;),
OR(
ISPICKVAL(CRM_Month__c, &apos;OCTOBER&apos;),
ISPICKVAL(CRM_Month__c, &apos;NOVEMBER&apos;),
ISPICKVAL(CRM_Month__c, &apos;DECEMBER&apos;)
)
),
AND(
!ISPICKVAL(CRM_KPI__r.CRM_Quarter__c, &apos;4&apos;),
OR(
ISPICKVAL(CRM_Month__c, &apos;JANUARY&apos;),
ISPICKVAL(CRM_Month__c, &apos;FEBRUARY&apos;),
ISPICKVAL(CRM_Month__c, &apos;MARCH&apos;)
)
)
)
)</errorConditionFormula>
        <errorDisplayField>CRM_Month__c</errorDisplayField>
        <errorMessage>Please select Month according to KPI&apos;s selected quarter.</errorMessage>
    </validationRules>
