} 
else{ 
sforce.apex.execute(&quot;JacksonApiCalling&quot;,&quot;updateError&quot;,{apilog:&quot;{!Diary_Notes__c.Api_Log__c}&quot;,entity:&quot;DiaryNote&quot;,recordID:&quot;{!Diary_Notes__c.Id}&quot;}); 
location.reload(true); 
}</url>
