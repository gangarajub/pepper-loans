        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 
if(&apos;{!Diary_Notes__c.AssigntoLoggedIn__c}&apos;==&apos;1&apos;){
if(&apos;{!Diary_Notes__c.Submitted_to_ESB__c}&apos;==&apos;1&apos;){
alert(&apos;Diary Note already submitted.&apos;);
}
else{
var valid=sforce.apex.execute(&quot;JacksonApiCalling&quot;,&quot;submitDiaryNote&quot;,{diaryNoteID:&quot;{!Diary_Notes__c.Id}&quot;});
if(valid==&apos;false&apos;){
alert(&quot;Property is not registered or Loan contract is not active or does not belong to COC Lenders.&quot;);
}
 else{
     alert(&quot;Diary Note submitted&quot;);
     location.reload(true);
   }
  }
}
else
{
alert(&apos;Property insurance associated with the diary note must be assigned to you before invoking the same.&apos;);
