    <validationRules>
        <fullName>AgentClosedDateGreaterThanDateAppointed</fullName>
        <active>true</active>
        <description>Agent Close Date Should be greater than Date Appointed.</description>
        <errorConditionFormula>OR(AND(ISNULL(ScCollection_Date_Appointed__c),NOT(ISNULL(ScCollection_Agent_Close_Date__c))),ScCollection_Date_Appointed__c &gt; ScCollection_Agent_Close_Date__c)</errorConditionFormula>
        <errorDisplayField>ScCollection_Agent_Close_Date__c</errorDisplayField>
        <errorMessage>Agent Close Date Should be greater than Date Appointed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EitherReleaseOfAssetOrRedemptionDate</fullName>
        <active>true</active>
        <description>Both Release of Asset and Date of Redemption cannot be filled.</description>
        <errorConditionFormula>AND( NOT(ISNULL(ScCollection_Release_of_Asset__c)) , NOT(ISNULL(Date_of_Redemption__c)))</errorConditionFormula>
        <errorMessage>Both Release of Asset and Date of Redemption cannot be filled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReleaseOfAssetOrRedemptionDateGreater</fullName>
        <active>true</active>
        <description>Repossession date cannot be greater than Release of Asset or Date of Redemption.</description>
        <errorConditionFormula>OR(AND(ISNULL(ScCollection_Repossession_Date__c),OR(NOT(ISNULL(ScCollection_Release_of_Asset__c)) , NOT(ISNULL(Date_of_Redemption__c)))), ScCollection_Repossession_Date__c &gt;  ScCollection_Release_of_Asset__c ,ScCollection_Repossession_Date__c &gt;  Date_of_Redemption__c)</errorConditionFormula>
        <errorMessage>Release of Asset or Date of Redemption should be greater than Repossession Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RestrictMoreThanOneInProgressRecord</fullName>
        <active>true</active>
        <description>This rule is setup to restrict more than one in progress recovery record against property.</description>
        <errorConditionFormula>AND(ISNEW(), ScCollection_Property_Surrogate__r.Recovery_Record_Count__c &gt;= 1,  TEXT(ScCollection_Status__c) = &apos;In Progress&apos;)</errorConditionFormula>
        <errorMessage>You can&apos;t tag more than one in-progress recoveries against a property.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RestrictUpdationOfClosedRecovery</fullName>
        <active>true</active>
        <description>Validation for not being able to edit the record once the status is changed to &apos;Closed&apos;.</description>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(ScCollection_Status__c),&quot;Closed&quot;),NOT(ISNEW()))</errorConditionFormula>
        <errorMessage>You cannot edit the record once the status is changed to &apos;Closed&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SoldDateGreaterThanReleaseOfAsset</fullName>
        <active>true</active>
        <description>Sold Date should be greater than Release of Asset Date.</description>
        <errorConditionFormula>OR(AND(ISNULL(ScCollection_Release_of_Asset__c),NOT(ISNULL(ScCollection_Sold_Date__c))),ScCollection_Release_of_Asset__c &gt; ScCollection_Sold_Date__c)</errorConditionFormula>
        <errorDisplayField>ScCollection_Sold_Date__c</errorDisplayField>
        <errorMessage>Sold Date should be greater than Release of Asset Date.</errorMessage>
    </validationRules>
