    <validationRules>
        <fullName>CRM_Validation_to_check_value_change</fullName>
        <active>false</active>
        <errorConditionFormula>IF( AND( NOT(ISCHANGED(CRM_Status__c)),ISPICKVAL(CRM_Status__c, &apos;Completed&apos;), NOT($User.ProfileId = &quot;00e28000001sDhW&quot;) ,  
 OR( ISCHANGED( CRM_Target_January__c ) ,
 ISCHANGED(CRM_Target_February__c),
 ISCHANGED(CRM_Target_March__c),
 ISCHANGED( CRM_Target_April__c ),
 ISCHANGED( CRM_Target_May__c ),
 ISCHANGED( CRM_Target_June__c ),
 ISCHANGED( CRM_Target_July__c ),
 ISCHANGED(CRM_Target_August__c),
 ISCHANGED( CRM_Target_September__c ),
 ISCHANGED(CRM_Target_October__c),
 ISCHANGED(CRM_Target_November__c),
 ISCHANGED(CRM_Target_December__c)
 )  ) , true, false)</errorConditionFormula>
        <errorMessage>You cannot update Target, Please contact your manager.</errorMessage>
    </validationRules>
