	IF(
	  NOT( ISBLANK(ScCollection_Contract__c ) ) 
	  ,
		IF(
			ScCollection_Contract__r.ScCollection_Source_System_Name__c == &quot;MORTGAGE&quot;
		,
			left( ScCollection_Contract__r.Name ,find(&apos;_&apos;, ScCollection_Contract__r.Name)-1)
