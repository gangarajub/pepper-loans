     <script>
     function checkedValue(strVal)
     {
        
         //alert($("#"+strVal+"").is(":checked"));
         $("#"+strVal+"").removeClass('checked');
         
         var strChecked = $("#"+strVal+"").is(":checked");
         callActionFun(strVal,strChecked);
     
     
     }
     
     function disableCheckBox()
     {
          
         $(".communicationChecked").each(function(e)
         {
          
         
           if('{!isSection3Changed}' == 'true')
           {
                 $(this).attr("disabled", false);
                  $(".spanComm ").removeClass("chkboxDis");
           }
           else
           {
              $(this).attr("disabled", "disabled");
              $(".spanComm ").addClass("chkboxDis");
           }
            
         });
     }
     
     $(document).ready(function()
     {
         
         disableCheckBox();
       
     });
     $(document).on("keydown", ".pwd", function (e) 
    {
        return e.which != 32;
    });
    
    function passwordvalidations()
    {
        var strCurrentPwd = $( ".currntpwd " ).val();
        var strNewPwd = $( ".newpwd" ).val();
        var strConfirmPwd = $( ".cnfpwd" ).val();
        
         if(strConfirmPwd =='')
         {
            $( ".pwdMsg" ).text("");
            return false;
         }      
        if(!(isNaN(strNewPwd) && strNewPwd.length >= 8 && strNewPwd.match(/[a-z]/i)))
        {
             $(".pwdMsg").show(); 
             
            
            $( ".pwdMsg" ).text("Password is incorrect or does not meet our criteria. Please re-try");
            return false;
        }
        
        if(strCurrentPwd != '' && strNewPwd !='' && strConfirmPwd !=''&& strCurrentPwd == strNewPwd)
        {
           
            $(".pwdMsg").show(); 
            $( ".pwdMsg" ).text("Your password shouldn\'t be your old password");
            return false;
        }
        else if(strConfirmPwd !='' && strNewPwd !='' && strConfirmPwd  != strNewPwd)
        {
             
            $(".pwdMsg").show(); 
            $( ".pwdMsg " ).html("Your password doesn\'t match");
            return false;
        }
        else
        {
            
            $( ".pwdMsg" ).text("");
        }
        return true;
    }
    
    
   function  checkData(theform,strclassName)
    {   
    
        $( ".pwdMsg" ).text("");           
        var myForm = document.getElementById(theform);         
        var boolFlag = true;          
        for (var i = 0; i < myForm.elements.length; i++) 
        {
          if(myForm.elements[i].getAttribute("type") == "hidden") continue;
          
          var $this = $(document.getElementById(myForm.elements[i].id));
          if(!$this.hasClass(strclassName)) continue;
          
          var fieldFlag = checkFieldValidation(myForm.elements[i].id);
       //   alert('val is'+fieldFlag);
          if(boolFlag)
          {
            boolFlag =  fieldFlag;
          }
        }

        //checking radio button is checked or unchecked
 
       var radioCheckFlag = radioCheck(true, theform);
       if(boolFlag)
       {
            boolFlag = radioCheckFlag;
       }
       
        if(strclassName =='ispassword')
        {
           
           boolFlag = passwordvalidations();
          
          
        }
    //   alert(boolFlag);
         $('.tmpMsg').hide();
        return boolFlag; 
         
    }
    
    
        
       
    
    
    function hidemsg()
    {
      
        $(".pwderror").hide();
         
    }
    
    function hidemsg1()
    {
      
        $(".pwdMsg").hide();
         
    }
   
     function disableCopyPaste()
    {
        hidemsg();
        var controls = $(".disablecopy ");
        controls.bind("paste", function () {
            return false;
        });
        controls.bind("drop", function () {
            return false;
        });
        controls.bind("cut", function () {
            return false;
        });
        controls.bind("copy", function () {
            return false;
        });
    }
     
     
     function checkUser()
     {
          if(('{!checkUserId}')=='true')
            {
                 $('.errorMsg').show();
                 return false;
            }
     
     }
    </script>
